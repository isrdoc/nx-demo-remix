{
  "name": "nx-demo-remix",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "remix setup node",
    "demo:prisma:schema": "yarn run demo:prisma:merge && yarn run demo:prisma:generate",
    "demo:prisma:generate": "cd packages/demo && npx prisma generate",
    "demo:prisma:merge": "cd packages/demo && yarn run prisma-merge -b 'prisma/base.prisma' -o 'prisma/schema.prisma' -s '**/*.prisma'",
    "demo:prisma:seed:notifications": "ts-node packages/demo/app/routes/notifications.seed.ts"
  },
  "prisma": {
    "seed": "yarn run demo:prisma:seed:notifications"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "express": "4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nrwl/cli": "13.10.1",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/express": "^13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/js": "13.10.1",
    "@nrwl/linter": "13.10.2",
    "@nrwl/node": "13.10.2",
    "@nrwl/remix": "13.7.1",
    "@nrwl/workspace": "13.10.1",
    "@remix-run/dev": "^1.0.6",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.2.3",
    "nx": "13.10.1",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "^3.12.0",
    "prisma": "^3.12.0",
    "prisma-merge": "^0.2.0",
    "ts-jest": "27.0.5",
    "ts-node": "^10.7.0",
    "typescript": "~4.6.2"
  }
}
